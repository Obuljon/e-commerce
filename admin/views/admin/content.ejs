
<div class="app-content content">
    <div class="content-wrapper">
      <div class="content-wrapper-before"></div>
      <div class="content-header row">
        <div class="content-header-left col-md-4 col-12 mb-2">
          <h3 class="content-header-title">Tables</h3>
        </div>
        <div class="content-header-right col-md-8 col-12">
          <div class="breadcrumbs-top float-md-right">
            <div class="breadcrumb-wrapper mr-1">
              <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/">Home</a>
                </li>
                <li class="breadcrumb-item active">Tables
                </li>
              </ol>
            </div>
          </div>
        </div>
      </div>
      <div class="content-body"><!-- Basic Tables start -->
<div class="row">
  <div class="col-12">
      <div class="card">
          <div class="card-header">
              <h4 class="card-title">Basic Tables</h4>
          </div>
          <div class="card-content collapse show">
              <div class="card-body">
                  <p class="card-text"></p>
                  <p></p>
                  <div class="table-responsive">
                      <table class="table">
                          <thead>
                              <tr>
                                  <th>#</th>
                                  <th>Name</th>
                                  <th>lastname</th>
                                  <th>email</th>
                                  <th>message</th>
                                  <td>del</td>
                              </tr>
                          </thead>
                          <tbody>
                            <% if (content && content.length > 0) {%>
                            <% content.forEach((e, i) => {%>
                                <tr class="<%if(!e.wasseen){%>table-warning<%}%>">
                                  <th><%=i + 1%></th>
                                  <td><%= e.name%></td>
                                  <th><%= e.lastname%></th>
                                  <td><%= e.email%></td>
                                  <td><!-- Button trigger modal -->
                                    <button type="button" class="btn btn-icon btn-info mr-1" data-toggle="modal" data-target="#exampleModal-<%=i%>">
                                      content
                                    </button>
                                    
                                    <!-- Modal -->
                                    <div class="modal fade" id="exampleModal-<%=i%>" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                      <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                          <div class="modal-header">
                                            <h5 class="modal-title" id="exampleModalLabel"><%=e.subject%></h5>
                                          </div>
                                          <div class="modal-body">
                                            <p><%=e.message%></p>
                                            </div>
                                          </div>
                                        </div>
                                      </div>
                                    </td>
                                    <td><a class="btn btn-icon btn-danger mr-1" href="/contentdelete<%=e.id%>"><i class="la la-trash-o"></i></a></td>
                                  </thead>
                            <%}) %> 
                          <% }%>
                          </table>
                          <section id="icons-buttons">
                            <div class="row">
                                <div class="col-lg-12 col-md-12">
                                    <div class="card">
                                        <div class="card-content collapse show">
                                            <div class="card-body">
                                                <div class="form-group">
                                                  
                                                  <%pagenetion.forEach(e => {%>
                                                    <a href="/content<%=e%>" class="btn btn-success mr-1" style="margin-top: 1%; color: antiquewhite;"><%=e%></a>
                                                    <%})%>
                                                
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                  </div>
              </div>
          </div>
      </div>
  </div>
</div>
<!-- Basic Tables end -->
      </div>
    </div>
  </div>
  <!-- ////////////////////////////////////////////////////////////////////////////-->
